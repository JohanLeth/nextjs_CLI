{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node LTS",
    "_version": "18.1.0",
    "compilerOptions": {
        "lib": ["es2023"],
        "module": "Node16",
        "target": "ES2022",
        "strict": true,
        "noUncheckedIndexedAccess": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node16"
    },
    "include": ["src"]
}
